   captionText.innerHTML = dots[slideIndex - 1].alt;\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2MA;AACA;EACAA,OADA,qBACA;IACA;IACAC,uBAFA,CAIA;;IACA;MACAA;IACA,CAPA,CASA;;;IACA;MACAA;IACA;;IAEA;MACA;MACA;MACA;MACA;;MACA;QACAC;MACA;;MACA;QACAA;MACA;;MACA;QACAC;MACA;;MACA;QACAC;MACA;;MACAD;MACAC;MACAC;IACA;EACA;AApCA","names":["mounted","showSlides","slideIndex","slides","dots","captionText"],"sourceRoot":"src/components","sources":["thirdSection.vue"],"sourcesContent":["<template>\r\n<section class=\"section-third-info\">\r\n    <div class=\"section-third-info-left\">\r\n        <div class=\"section-third-info-left-header\">This is my first work done in wordpress</div>\r\n        <div class=\"section-third-info-left-desc\">The site was made using the elementor plugin</div>\r\n        <div class=\"section-third-info-left-btn hero-video-text-btn\">Learn more</div>\r\n    </div>\r\n    <div class=\"section-third-info-right\">\r\n        <div class=\"container\">\r\n\r\n            <!-- Full-width images with number text -->\r\n            <div class=\"mySlides\">\r\n                <div class=\"numbertext\">1 / 6</div>\r\n                <img src=\"../img/1.png\" style=\"width:100%\">\r\n            </div>\r\n\r\n            <div class=\"mySlides\">\r\n                <div class=\"numbertext\">2 / 6</div>\r\n                <img src=\"../img/1 (2).png\" style=\"width:100%\">\r\n            </div>\r\n\r\n            <div class=\"mySlides\">\r\n                <div class=\"numbertext\">3 / 6</div>\r\n                <img src=\"../img/1 (3).png\" style=\"width:100%\">\r\n            </div>\r\n\r\n            <a class=\"prev\" onclick=\"plusSlides(-1)\">&#10094;</a>\r\n            <a class=\"next\" onclick=\"plusSlides(1)\">&#10095;</a>\r\n\r\n            <div class=\"caption-container\">\r\n                <p id=\"caption\"></p>\r\n            </div>\r\n\r\n            <div class=\"row\">\r\n                <div class=\"column\">\r\n                    <img class=\"demo cursor\" src=\"../img/1.png\" style=\"width:100%\" onclick=\"currentSlide(1)\" alt=\"The Woods\">\r\n                </div>\r\n                <div class=\"column\">\r\n                    <img class=\"demo cursor\" src=\"../img/1 (2).png\" style=\"width:100%\" onclick=\"currentSlide(2)\" alt=\"Cinque Terre\">\r\n                </div>\r\n                <div class=\"column\">\r\n                    <img class=\"demo cursor\" src=\"../img/1 (3).png\" style=\"width:100%\" onclick=\"currentSlide(3)\" alt=\"Mountains and fjords\">\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</section>\r\n</template>\r\n\r\n<style>\r\n* {\r\n    box-sizing: border-box;\r\n}\r\n\r\n/* Position the image container (needed to position the left and right arrows) */\r\n.container {\r\n    position: relative;\r\n}\r\n\r\n/* Hide the images by default */\r\n.mySlides {\r\n    display: none;\r\n}\r\n\r\n/* Add a pointer when hovering over the thumbnail images */\r\n.cursor {\r\n    cursor: pointer;\r\n}\r\n\r\n/* Next & previous buttons */\r\n.prev,\r\n.next {\r\n    cursor: pointer;\r\n    position: absolute;\r\n    top: 40%;\r\n    width: auto;\r\n    padding: 16px;\r\n    margin-top: -50px;\r\n    color: white;\r\n    font-weight: bold;\r\n    font-size: 20px;\r\n    border-radius: 0 3px 3px 0;\r\n    user-select: none;\r\n    -webkit-user-select: none;\r\n}\r\n\r\n/* Position the \"next button\" to the right */\r\n.next {\r\n    right: 0;\r\n    border-radius: 3px 0 0 3px;\r\n}\r\n\r\n/* On hover, add a black background color with a little bit see-through */\r\n.prev:hover,\r\n.next:hover {\r\n    background-color: rgba(0, 0, 0, 0.8);\r\n}\r\n\r\n/* Number text (1/3 etc) */\r\n.numbertext {\r\n    color: #f2f2f2;\r\n    font-size: 12px;\r\n    padding: 8px 12px;\r\n    position: absolute;\r\n    top: 0;\r\n}\r\n\r\n/* Container for image text */\r\n.caption-container {\r\n    text-align: center;\r\n    background-color: #222;\r\n    padding: 2px 16px;\r\n    color: white;\r\n}\r\n\r\n.row:after {\r\n    content: \"\";\r\n    display: table;\r\n    clear: both;\r\n}\r\n\r\n/* Six columns side by side */\r\n.column {\r\n    float: left;\r\n    width: 16.66%;\r\n}\r\n\r\n/* Add a transparency effect for thumnbail images */\r\n.demo {\r\n    opacity: 0.6;\r\n}\r\n\r\n.active,\r\n.demo:hover {\r\n    opacity: 1;\r\n}\r\n\r\n.section-third-info {\r\n    display: flex;\r\n    height: 100vh;\r\n    min-height: 700px;\r\n}\r\n\r\n.section-third-info-left {\r\n    width: 50%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n    justify-content: center;\r\n    padding: 120px 40px 75px 6.6%;\r\n}\r\n\r\n.section-third-info-right {\r\n    width: 50%;\r\n}\r\n\r\n.section-third-info-left-header {\r\n    font-size: 40px;\r\n    line-height: 64px;\r\n    letter-spacing: -.02em;\r\n    font-weight: 700;\r\n}\r\n\r\n.section-third-info-left-desc {\r\n    margin-top: 15px;\r\n    font-size: 24px;\r\n    line-height: 36px;\r\n    letter-spacing: -.03em;\r\n    font-weight: 400;\r\n    color: #828282;\r\n}\r\n\r\n.hero-video-text-btn {\r\n    margin-top: 30px;\r\n    display: inline-block;\r\n    padding: 14px 34px;\r\n    font-size: 16px;\r\n    border: 2px solid #fd0202;\r\n    border-radius: 4px;\r\n    -webkit-user-select: none;\r\n    -moz-user-select: none;\r\n    -ms-user-select: none;\r\n    user-select: none;\r\n    text-align: center;\r\n    transition: background .3s ease-in-out;\r\n    cursor: pointer;\r\n}\r\n\r\n.hero-video-text-btn:hover {\r\n    background: #fd0202;\r\n}\r\n\r\n.section-third-info-right {\r\n    background-image: url('../img/1.png');\r\n    background-size: cover;\r\n    background-repeat: no-repeat;\r\n    background-position: 50%, 50%;\r\n    z-index: -2;\r\n\r\n}\r\n</style>\r\n\r\n<script>\r\n// eslint-disable-next-line no-unused-vars\r\nexport default {\r\n    mounted() {\r\n        var slideIndex = 1;\r\n        showSlides(slideIndex);\r\n\r\n        // Next/previous controls\r\n        function plusSlides(n) {\r\n            showSlides(slideIndex += n);\r\n        }\r\n\r\n        // Thumbnail image controls\r\n        function currentSlide(n) {\r\n            showSlides(slideIndex = n);\r\n        }\r\n\r\n        function showSlides(n) {\r\n            var i;\r\n            var slides = document.getElementsByClassName(\"mySlides\");\r\n            var dots = document.getElementsByClassName(\"demo\");\r\n            var captionText = document.getElementById(\"caption\");\r\n            if (n > slides.length) {\r\n                slideIndex = 1\r\n            }\r\n            if (n < 1) {\r\n                slideIndex = slides.length\r\n            }\r\n            for (i = 0; i < slides.length; i++) {\r\n                slides[i].style.display = \"none\";\r\n            }\r\n            for (i = 0; i < dots.length; i++) {\r\n                dots[i].className = dots[i].className.replace(\" active\", \"\");\r\n            }\r\n            slides[slideIndex - 1].style.display = \"block\";\r\n            dots[slideIndex - 1].className += \" active\";\r\n            captionText.innerHTML = dots[slideIndex - 1].alt;\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}